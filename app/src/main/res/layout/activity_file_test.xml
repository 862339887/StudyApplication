<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.FileTestActivity">

    <Button
        android:id="@+id/write_file_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="write_file_btn" />

    <Button
        android:id="@+id/delete_file_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="delete_file_btn"

        />

    <Button
        android:id="@+id/println_file_path_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="println_file_path_btn" />

</LinearLayout>